{
  "dist": {
    "files": [{
      "dot": true,
      "src": [
        ".tmp",
        "<%= yeoman.dist %>/*",
        "!<%= yeoman.dist %>/.git*",
        "!<%= yeoman.dist %>/index.html"
      ]
    }]
  },
